{
  "name": "@ngx-env/builder",
  "version": "21.0.1",
  "description": "Easily inject environment variables into your Angular applications",
  "author": "chihab <chihab@gmail.com>",
  "homepage": "https://github.com/chihab/ngx-env/tree/main/packages/angular",
  "license": "MIT",
  "keywords": [
    "angular",
    "dotenv",
    "environment",
    "builder",
    "schematics"
  ],
  "files": [
    "dist",
    "builders.json",
    "collection.json",
    "README.md",
    "runtime.mjs"
  ],
  "builders": "builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc && ts-node tools/schema-copy-run.ts",
    "copy-dist": "ts-node tools/schema-dist.ts"
  },
  "dependencies": {
    "glob": "^10.4.5"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular-devkit/architect": "^0.2100.0",
    "@angular-devkit/build-angular": "^21.0.2",
    "@angular-devkit/core": "^21.0.0",
    "@angular-devkit/schematics": "^21.0.0",
    "@dotenv-run/core": "workspace:^1.3.7",
    "@dotenv-run/webpack": "workspace:^1.5.2",
    "cpy": "^8.1.2",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "webpack": "5.99.8"
  },
  "peerDependencies": {
    "@angular/build": "^21.0.0"
  },
  "optionalDependencies": {
    "@dotenv-run/webpack": "workspace:^1.5.2"
  }
}
