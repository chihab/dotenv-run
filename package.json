{
  "name": "dotenv-run",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev --parallel --no-cache --filter='./packages/*'",
    "test": "turbo test --filter='./packages/*'",
    "build": "turbo build  --filter='./packages/*'",
    "build:docs": "turbo build  --filter='./docs'",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.3",
    "concurrently": "^8.2.2",
    "husky": "^8.0.0",
    "turbo": "^1.12.4",
    "typescript": "^5.4.0",
    "vitest": "^0.33.0"
  },
  "workspaces": {
    "packages": [
      "examples",
      "packages/*",
      "docs"
    ]
  }
}
